<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.booking_app">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".WelcomeScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Admin.BookedSlots" />
        <activity android:name=".Admin.AddRoomActivity" />
        <activity android:name=".Admin.AdminEventDetails" />
        <activity android:name=".Admin.AdminMain" />
        <activity android:name=".User.FormActivity" />
        <activity android:name=".User.SortRoomActivity" />
        <activity android:name=".User.CategoryActivity" />
        <activity android:name=".SignUp" />
        <activity android:name=".MainActivity">
        </activity>
    </application>

</manifest>